{
  "name": "minio",
  "version": "7.0.34",
  "description": "S3 Compatible Cloud Storage client",
  "source": "./src/main/minio.mjs",
  "main": "./dist/cjs/minio.cjs",
  "module": "./dist/esm/minio.mjs",
  "types": "./src/main/minio.d.ts",
  "scripts": {
    "format": "prettier -w .",
    "format-check": "prettier --list-different .",
    "prebuild": "rimraf ./dist/",
    "build": "rollup -c rollup.config.cjs",
    "test": "mocha",
    "lint": "eslint --ext js,mjs,cjs,ts,mts,cts ./src/",
    "lint-fix": " eslint --ext js,mjs,cjs,ts,mts,cts ./src/ --fix",
    "prepublishOnly": "npm test",
    "functional": "mocha src/test/functional/functional-tests.mjs",
    "tsc": "tsc",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "exports": {
    ".": {
      "types": "./src/main/minio.d.ts",
      "require": "./dist/cjs/minio.cjs",
      "default": "./dist/esm/minio.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "package.json",
    "./dist/",
    "./src/",
    "LICENSE",
    "README.md",
    "README_zh_CN.md",
    "MAINTAINERS.md"
  ],
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "endOfLine": "lf",
    "semi": false
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,cjs,mjs,ts,cts,mts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minio/minio-js.git"
  },
  "author": {
    "name": "MinIO, Inc.",
    "email": "",
    "url": "https://min.io"
  },
  "engines": {
    "node": ">8  <=19"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/minio/minio-js/issues",
    "mail": ""
  },
  "homepage": "https://github.com/minio/minio-js#readme",
  "dependencies": {
    "async": "^3.1.0",
    "block-stream2": "^2.0.0",
    "browser-or-node": "^1.3.0",
    "buffer-crc32": "^0.2.13",
    "es6-error": "^4.1.1",
    "fast-xml-parser": "^4.1.3",
    "ipaddr.js": "^2.0.1",
    "json-stream": "^1.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.14",
    "mkdirp": "^0.5.1",
    "query-string": "^7.1.1",
    "through2": "^3.0.1",
    "web-encoding": "^1.1.5",
    "xml": "^1.0.0",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/browser-or-node": "^1.3.0",
    "@types/lodash": "^4.14.192",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chai": "^4.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^9.2.0",
    "mocha-steps": "^1.1.0",
    "nock": "^13.2.2",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "source-map-support": "^0.5.13",
    "split-file": "^2.2.2",
    "superagent": "^5.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "api",
    "amazon",
    "minio",
    "cloud",
    "s3",
    "storage"
  ]
}
